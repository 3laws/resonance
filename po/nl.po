msgstr "muziek;geluid;speler;media;audio;afspeellijst;playlist;"

#: data/io.github.nate_xyz.Resonance.appdata.xml.in:7
msgid "Harmonize your listening experience"
msgstr "Geniet van alle harmonieën"

